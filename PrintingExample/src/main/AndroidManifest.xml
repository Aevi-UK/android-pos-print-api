<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.aevi.example.print">

    <uses-permission android:name="com.aevi.permission.NGS_PRINT_SERVICE"/>

    <application
        android:icon="@drawable/launcher_icon"
        android:label="@string/app_name"
        android:noHistory="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".PrintingActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".PrintPreviewActivity"
            android:theme="@style/AppTheme"/>

        <meta-data
            android:name="application-type"
            android:value="vaa_merchant"/>

    </application>
</manifest>
